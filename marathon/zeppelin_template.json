{
    "id": "/zeppelin",
    "cmd": "sed \"s#<value>8080</value>#<value>$PORT0</value>#\" < conf/zeppelin-site.xml.template > conf/zeppelin-site.xml && sed -i \"s#<value>-1</value>#<value>$PORT1</value>#\" conf/zeppelin-site.xml && SPARK_HOME_TGZ=$(ls ${MESOS_SANDBOX}/spark-*.tgz) SPARK_HOME=${SPARK_HOME_TGZ%.tgz} bin/zeppelin.sh start",
    "cpus": 1,
    "mem": 2048,
    "disk": 0,
    "instances": 1,
    "container": {
        "type": "DOCKER",
        "volumes": [],
        "docker": {
            "image": "brndnmtthws/zeppelin:0.6.0",
            "network": "HOST",
            "privileged": false,
            "parameters": [],
            "forcePullImage": false
        }
    },
    "env": {
        "SPARK_EXECUTOR_MEMORY": "8g",
        "ZEPPELIN_JAVA_OPTS": "-Dspark.cassandra.connection.host=node-0.cassandra.mesos,node-1.cassandra.mesos,node-2.cassandra.mesos -Dspark.mesos.coarse=true -Dspark.mesos.executor.home=/opt/spark/dist",
        "SPARK_CORES_MAX": "",
        "SPARK_MESOS_EXECUTOR_DOCKER_IMAGE": "mesosphere/spark:1.6.1-6"
    },
    "healthChecks": [
                     {
                     "protocol": "TCP",
                     "portIndex": 0,
                     "gracePeriodSeconds": 300,
                     "intervalSeconds": 60,
                     "timeoutSeconds": 15,
                     "maxConsecutiveFailures": 3,
                     "ignoreHttp1xx": false
                     }
                     ],
    "labels": {
        "DCOS_PACKAGE_RELEASE": "1",
        "DCOS_SERVICE_SCHEME": "http",
        "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
        "HAPROXY_GROUP": "external",
        "HAPROXY_0_VHOST": "elb_hostname",
        "HAPROXY_1_VHOST": "elb_hostname",
        "DCOS_PACKAGE_METADATA": "",
        "DCOS_PACKAGE_REGISTRY_VERSION": "2.0",
        "DCOS_SERVICE_NAME": "zeppelin",
        "DCOS_SERVICE_PORT_INDEX": "0",
        "DCOS_PACKAGE_VERSION": "0.5.6",
        "DCOS_PACKAGE_NAME": "zeppelin",
        "DCOS_PACKAGE_IS_FRAMEWORK": "true"
    },
    "portDefinitions": [
                        {
                        "port": 10102,
                        "protocol": "tcp",
                        "labels": {}
                        },
                        {
                        "port": 10103,
                        "protocol": "tcp",
                        "labels": {}
                        }
                        ],
    "fetch": [
              {
              "uri": "https://downloads.mesosphere.io/spark/assets/spark-1.6.1-1.tgz",
              "extract": true,
              "executable": false,
              "cache": false
              }
              ]
}
